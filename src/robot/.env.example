# Environment variables for e-puck2 robot server
# Copy this file to .env and modify as needed

# Server configuration
# ROBOT_HOST=  # Leave empty for auto-detection (will detect 192.168.0.xxx automatically)
ROBOT_PORT=8765

# Logging configuration
LOG_LEVEL=INFO
LOG_FILE=logs/robot_server.log
LOG_MAX_BYTES=10485760
LOG_BACKUP_COUNT=5
